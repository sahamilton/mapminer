{
    "name": "sahamilton/mapminerv25",
    "description": "People Ready Mapminer.",
    "keywords": [
        "trueblue",
        "people ready",
        "mapminer"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.18",
        "baum/baum": "~1.1",
        "bugsnag/bugsnag-laravel": "^2.4",
        "dionera/laravel-beanstalkd-ui": "^1.2",
        "elasticsearch/elasticsearch": "^5.2",
        "fabpot/goutte": "^3.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "jeroendesloovere/vcard": "^1.5",
        "laravel/framework": "5.7.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.3.0",
        "maatwebsite/excel": "~3",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "pda/pheanstalk": "^3.1",
        "smalot/pdfparser": "^0.10.0",
        "tamayo/laravel-scout-elastic": "^3.0",
        "toin0u/geocoder-laravel": "^4.0",
        "dompdf/dompdf": "~0.6.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "nicolaslopezj/searchable": "1.*",
        "mccool/laravel-auto-presenter": "^7.0",
        "eyewitness/eye": "^2.0",
        "askedio/laravel-soft-cascade": "5.6",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "fideloper/proxy": "^4.0",
        "jjaffeux/postmark-inbound-php": "^3.0",
        "lab404/laravel-impersonate": "^1.2",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laracasts/generators": "^1.1",
        "filp/whoops": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
