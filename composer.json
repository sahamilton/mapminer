{
    "name": "sahamilton/mapminerv25",
    "description": "People Ready Mapminer.",
    "keywords": [
        "trueblue",
        "people ready",
        "mapminer"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.18",
        
        "bugsnag/bugsnag-laravel": "^2.4",
        "elasticsearch/elasticsearch": "^5.2",
        "fabpot/goutte": "~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "jeroendesloovere/vcard": "^1.5",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "laravel/ui": "^1.0",
        "maatwebsite/excel": "^3.1",
        "watson/aggregate": "^2.0",
        "smalot/pdfparser": "^0.14.0",
        "toin0u/geocoder-laravel": "^4.0",
        "dompdf/dompdf": "~0.6.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "nicolaslopezj/searchable": "1.*",
        "mccool/laravel-auto-presenter": "^7.4",
        "askedio/laravel-soft-cascade": "^6.0",
        "fideloper/proxy": "^4.0",
        "jjaffeux/postmark-inbound-php": "^3.0",
        "lab404/laravel-impersonate": "^1.3",
        "intervention/image": "^2.4",
        "spatie/flysystem-dropbox": "^1.0",
        "jenssegers/agent": "^2.6",
        "predis/predis": "^1.1",
        "doctrine/dbal": "2.10.4",
        "pda/pheanstalk": "^4.0",
        "dionera/laravel-beanstalkd-ui": "^2.0",
        "livewire/livewire": "^1.0",
        "spatie/laravel-fractal": "^5.7",
        "awobaz/compoships": "^1.1",
        "morrislaptop/laravel-queue-clear": "^1.2",
        "laravel/helpers": "^1.2",
        "spatie/laravel-uptime-monitor": "^3.8"
    },
    "require-dev": {
        "facade/ignition": "^1.0",
        "fzaninotto/faker":"dev-master",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "barryvdh/laravel-debugbar": "^3.2",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "ext-pcntl": "7.1",
            "ext-posix": "7.1"
        }
    }
}
